<h1>Log-in</h1>

<head>
  <link rel="stylesheet" href="app/stylesheets/sessions.css">
</head>
<body>
  <%= form_tag sessions_path do %>
  <div class="form">
    <%= label_tag "E-mail:"%><br />
    <%= text_field_tag :email%><br />
    <%= label_tag "Senha:"%><br />
    <%= password_field_tag :password%><br />

    <%=submit_tag "Log In" %>
  </div>
  <% end %>
</body>